{{!
    categoryInfoList: [{ href: string, title: string }]
}}

<section class="row">
  {{# categoryInfoList}}
  <div class="col-lg-4 col-6 pb-3">
    <div class="card category-info">
      <div class="card-body">
        <div class="card-title">
          <h4 class="category-title"><a href="{{href}}">{{title}}</a></h4>
        </div>
      </div>
    </div>
  </div>
  {{/ categoryInfoList}}
</section>